    "webpack-merge": "^5.8.0"
  },
  "build": {
    "appId": "io.github.xcodebuild.iproxy",
    "mac": {
      "category": "",
      "icon": "./vendor/files/icon.icns",
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "./vendor/files/icon.ico",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": "AppImage",
      "icon": "./verdor/files/icon.png"
    },
    "files": [
      "dist/*.js",
      "dist/*.html",
      "dist/files.zip",
      "package.json",
      "main.js"
    ],
    "directories": {
      "output": "release"
    }
  }
